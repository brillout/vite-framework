{
  "name": "@brillout/vite-framework",
  "version": "0.0.1",
  "description": "Example of a framework built on top of Vite + vite-plugin-ssr",
  "exports": {
    ".": {
      "node": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./_default.page.client": {
      "node": "./dist/_default.page.client.js",
      "import": "./dist/_default.page.client.js"
    },
    "./_default.page.server": {
      "node": "./dist/_default.page.server.js",
      "import": "./dist/_default.page.server.js"
    }
  },
  "devDependencies": {
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.4.2",
    "vite": "^2.5.3",
    "vite-plugin-ssr": "^0.2.13"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "vite": "^2.2.3",
    "vite-plugin-ssr": "^0.2.0"
  },
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "prepublishOnly": "npm run build"
  },
  "repository": "github:brillout/vite-framework",
  "publishConfig": {
    "access": "public"
  }
}
